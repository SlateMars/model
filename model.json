{
    "model_name": "AI_Essay_Detector",
    "description": "A fine-tuned model to detect if an essay is about Artificial Intelligence.",
    "architecture": "BERT",
    "hyperparameters": {
      "num_layers": 12,
      "hidden_size": 768,
      "num_attention_heads": 12,
      "vocab_size": 30522,
      "max_length": 512,
      "dropout_prob": 0.1,
      "learning_rate": 2e-5,
      "batch_size": 32,
      "num_epochs": 3
    },
    "training_data": {
      "source": "AI_Essay_Corpus",
      "format": "text",
      "size": "10,000 essays",
      "preprocessing_steps": ["Tokenization", "Padding"]
    },
    "evaluation_metrics": {
      "accuracy": 0.92,
      "precision": 0.88,
      "recall": 0.94,
      "f1_score": 0.91
    }
  }
  